{
  "name": "peibin/laravel-coordinator",
  "description": "Peibin coordinator component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "laravel",
    "pool"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "peibin/laravel-contracts": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Coordinator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Coordinator\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
